// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "sqlite"
    url      = env("DATABASE_URL")
}

model Card {
    id Int @id @default(autoincrement())
    era String
    type String
    shop String
    name String @unique
    imgUrl String @unique
    status String
}

model User {
    id Int @id @default(autoincrement())
    username @unique
}
